{
  "name": "biteship-api-node",
  "version": "1.0.0",
  "description": "Node.js SDK for integrating with the Biteship API",
  "main": "dist/biteship.cjs.js",
  "module": "dist/biteship.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/biteship.esm.js",
      "require": "./dist/biteship.cjs.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "keywords": [
    "biteship",
    "shipping",
    "logistics",
    "api",
    "sdk",
    "nodejs"
  ],
  "author": "Hasib <hsib19>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hsib19/biteship-api-node.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node-fetch": "^2.6.13",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "prettier": "3.8.1",
    "rollup": "^2.80.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
